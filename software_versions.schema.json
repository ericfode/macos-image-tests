{
    "title": "macOS Image",
    "type": "object",
    "properties": {
        "image": {
            "type": "object",
            "properties": {
                "build": {
                    "type": "string"
                },
                "date": {
                    "type": "string"
                },
                "sha1": {
                    "type": "string"
                }
            },
            "required": [
                "build",
                "date",
                "sha1"
            ]
        },
        "disk": {
            "type": "object",
            "properties": {
                "Filesystem": {
                    "type": "string"
                },
                "Size": {
                    "type": "string"
                },
                "Used": {
                    "type": "string"
                },
                "Avail": {
                    "type": "string"
                },
                "Capacity": {
                    "type": "string"
                }
            },
            "required": [
                "Filesystem",
                "Size",
                "Used",
                "Avail",
                "Capacity"
            ]
        },
        "os": {
            "type": "object",
            "properties": {
                "system_version": {
                    "type": "string"
                },
                "kernel_version": {
                    "type": "string"
                }
            },
            "required": [
                "system_version",
                "kernel_version"
            ]
        },
        "environment": {
            "type": "object",
            "properties": {
                "keyboard_layout": {
                    "type": "string"
                },
                "timezone": {
                    "type": "string"
                },
                "screensaver_ask_for_password": {
                    "type": "string"
                }
            },
            "required": [
                "keyboard_layout",
                "screensaver_ask_for_password",
                "timezone"
            ],
            "additionalProperties": false
        },
        "ruby": {
            "type": "object",
            "properties": {
                "system": {
                    "type": "string"
                },
                "installed": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "gems": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "version": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "name",
                            "version"
                        ]
                    }
                }
            },
            "required": [
                "installed",
                "system",
                "gems"
            ]
        },
        "tools": {
            "type": "object",
            "properties": {
                "python": {
                    "type": "string"
                },
                "python3": {
                    "type": "string"
                },
                "pip": {
                    "type": "string"
                },
                "pip3": {
                    "type": "string"
                },
                "xctool": {
                    "type": "string"
                },
                "java": {
                    "type": "string"
                },
                "parallel": {
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "xcode": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "license_accepted": {
                        "type": "boolean"
                    },
                    "tools_installed": {
                        "type": "boolean"
                    },
                    "simulators": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "version",
                    "build_version",
                    "simulators",
                    "license_accepted",
                    "tools_installed"
                ]
            }
        },
        "homebrew": {
            "type": "object"
        },
        "power_settings": {
            "type": "object",
            "required": [
                "sleep",
                "disksleep",
                "displaysleep",
                "hibernatemode"
            ]
        },
        "screen_locked": {
            "type":"boolean"
        },
        "command_line_tools": {
            "type": "object",
            "properties": {
                "package_id": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
                "volume": {
                    "type": "string"
                },
                "location": {
                    "type": "string"
                },
                "install_time": {
                    "type": "string"
                },
                "groups": {
                    "type": "string"
                }
            },
            "required": [
                "package_id",
                "version",
                "volume",
                "location",
                "install_time",
                "groups"
            ],
            "additionalProperties": false
        }
    },
    "required": [
        "image",
        "disk",
        "os",
        "environment",
        "ruby",
        "tools",
        "xcode",
        "homebrew",
        "power_settings",
        "command_line_tools",
        "screen_locked"
    ],
    "additionalProperties": false
}
